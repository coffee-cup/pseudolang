function quicksort(xs):
    if xs.length <= 1: return xs
    let pivot = xs[0]
    let less = [ x for x in xs[1:] if x <= pivot ]
    let greater = [ x for x in xs[1:] if x > pivot ]
    return quicksort(less) + [pivot] + quicksort(greater)

print( quicksort([73, 15, 42, 87, 3, 94, 58, 21, 61, 37]) )
